<?xml version="1.0" encoding="UTF-8"?>
<ContentPage
  xmlns="http://xamarin.com/schemas/2014/forms"
  xmlns:controls="clr-namespace:Xamarin.Forms.Labs.Controls;assembly=Xamarin.Forms.Labs"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  x:Class="Xamarin.Forms.Labs.Sample.Pages.Controls.ImageGalleryPage"
  Title="ImageGallery">
  <ContentPage.Content>
    <StackLayout>
      <controls:ImageGallery ItemsSource="{Binding Images}" > </controls:ImageGallery>
    </StackLayout>
  </ContentPage.Content>
</ContentPage>
