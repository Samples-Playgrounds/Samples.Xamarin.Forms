<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
	xmlns="http://xamarin.com/schemas/2014/forms" 
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
	x:Class="HolisticWare.XamarinForms.Mobile.Audio.AudioPlayer.ContentViewAudioPlayer"
	>
	<ContentView.Content>
		<StackLayout>
			<Picker 
				x:Name="pickerSongs"
				BindingContext="{Songs}"
				>
			</Picker>
			<Button
				x:Name="buttonPlay"
				Text="Play"
				>
			</Button>
		</StackLayout>
	</ContentView.Content>
</ContentView>
